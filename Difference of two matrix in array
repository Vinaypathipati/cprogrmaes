#include<stdio.h>
int main()
{
	int a[3][3],b[3][3],c[3][3];
	int i,j;
	for (i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			scanf("%d\t",&a[i][j]);
		}
	}
	for (i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			scanf("%d\t",&b[i][j]);	
		}
	}
	for (i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			c[i][j] =( a[i][j] - b[i][j]);
			
		}
	}
	printf("A matrix");
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			printf("%d\t",a[i][j]);
		}
		       printf("\n");
	}
		printf("B matrix");
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			printf("%d\t",b[i][j]);
		}
		       printf("\n");
	}
		printf("Difference of a matrix");
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			printf("%d\t",c[i][j]);
		}
		       printf("\n");
	}
}
