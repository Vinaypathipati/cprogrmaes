#include<stdio.h>

int lcm(int a,int b);

int main()
{
	int a,b,x;
	printf("enter a value");
	scanf("%d",&a);
	printf("enter b value");
	scanf("%d",&b);
	x=(a>b)?a:b;
	printf("lcm is %d",a,b,x);
}
int lcm(int a,int b)
{
	int x;
	while (x)
	{
		if(x%1==0 && x%2==0)
		{
			printf("lcm is %d",a,b,x);
			break;
		}
		++x;
	}
}
