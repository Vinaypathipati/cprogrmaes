#include<stdio.h>

void swap(int , int );

int main()
{

int a,b;
printf("enter the values of a,b\n");
scanf("%d%d",&a,&b);

  printf("before swapping: a =%d  and b=%d\n",a,b);
  
  swap (a,b);
  }
  void swap(int x ,int y)
  {
  int v;
  v=x;
  x=y;
  y=v;
  printf("after  swapping: a=%d and b=%d\n",x,y);
  }
  
